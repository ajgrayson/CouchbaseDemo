{
   "databases":{
      "default":{
         "server":"http://localhost:8091",
         "bucket":"default",
         "sync":`function(doc, oldDoc) {
  requireUser(doc.owner);
  if (oldDoc != null) {
        if (doc.owner != oldDoc.owner) {
            throw({forbidden: "Can't change owner"});
        }
    }
    requireAccess(doc.channels);
  channel(doc.channels);
}`
      }
   },
   "facebook" : {
      "register" : true
   }
}